{
  "name": "paycheck-perspective",
  "displayName": "Paycheck perspective",
  "version": "1.0.1",
  "description": "General job management extension, allows saving and storing job postings while proving general job seeking advice",
  "author": "Mia Damato, Taylor Cassidy, Gabriel QC",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test",
    "start": "tsc && node dist/index.js && node app.js"
  },
  "dependencies": {
    "browserify-fs": "^1.0.0",
    "mysql": "latest",
    "plasmo": "^0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.32",
    "@types/chrome": "0.0.245",
    "@types/node": "^20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "scripting",
      "activeTab",
      "tabs"
